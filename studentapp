pipeline {
    agent any
    stages {
        stage ('pull') {
            steps {
                git branch: 'master', url: 'https://github.com/shubhamkalsait/studentapp-ui.git'
            }
        }
        stage ('build') {
            steps {
                sh '/opt/apache-maven/bin/mvn clean package'
            }
        }
        stage ('test') {
            steps {
              
                 withSonarQubeEnv(installationName: 'sonar-server', credentialsId: 'sonar-token') {
                 sh '/opt/apache-maven/bin/mvn sonar:sonar -Dsonar.projectKey=studentapp -Dsonar.host.url=http://3.250.75.214:9000 -Dsonar.login=studentapp'
                   }
                
            }
        }
        stage ('deploy') {
            steps {
                echo 'deploy successs'
            }
        }
    }
}